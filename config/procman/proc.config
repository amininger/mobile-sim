// This file contains a list of all the processes which procman should
// run on the robot.

:process : object {
    host = "localhost";
    auto-restart = "true";
    restart-delay-ms = "500";
    auto-start = "true";
    name = "unknown";
}

// RobotDriver
proc# : process {
    cmd = "$JAVA $JAVA_OPTS_64 probcog.robot.robot.RobotDriver";
    name = "Robot Driver";
}

proc# : process {
    cmd = "$JAVA $JAVA_OPTS_64 probcog.robot.robot.EspeakDaemon";
    name = "Espeak Daemon";
}

// GamePadDriver
proc# : process {
    cmd = "$JAVA $JAVA_OPTS_64 april.util.GamePadDriver";
    name = "GamePadDriver";
}

Soar Navigation Control
proc# : process {
    cmd = "$JAVA $JAVA_OPTS_64 probcog.commands.CommandInterpreter";
    name = "Soar Command Controller";
}

// PIMU XXX /dev/pimu
proc# : process {
    cmd = "$JAVA $JAVA_OPTS_64 probcog.robot.pimu.PIMU -d /dev/ttyUSB1";
    name = "PIMU";
}

// PIMU Pose
proc# : process {
    cmd = "$JAVA $JAVA_OPTS_64 probcog.robot.pimu.PIMUPose";
    name = "PIMUPose";
}

// kinect
proc# : process {
    cmd = "$BIN_PATH/kinect";
    name = "Kinect Driver";
}
