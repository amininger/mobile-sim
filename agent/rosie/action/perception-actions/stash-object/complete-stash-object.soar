sp {stash-object*propose*complete-stash-object
   (state <s> ^name stash-object
              ^action-goal.achieved-subgoal.name move-object)
-->
   (<s> ^operator <o> + >)
   (<o> ^name complete-stash-object)
}

sp {stash-object*apply*complete-stash-object
   (state <s> ^name stash-object
              ^operator.name complete-stash-object
              ^action-goal <goal>)
-->
   (<goal> ^achieved true)
}