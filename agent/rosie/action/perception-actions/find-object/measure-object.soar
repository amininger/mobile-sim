sp {find-object*propose*create-measure-object-subgoal
   (state <s> ^name find-object
             -^moved-to-goal true
              ^matching-info <info>
              ^action-goal <goal>
             -^action-goal.action-goal)
   (<info> ^match-word <mw>
          -^good-match
           ^unknown-match <cand>)
   (<mw> ^word.property <prop>
         ^unknown-match <cand>)
   -{(<goal> ^achieved-subgoal <subgoal>)
     (<subgoal> ^parameters.object <cand>
                ^parameters.property <prop>)}
-->
   (<s> ^operator <o> + > =)
   (<o> ^name create-measure-object-subgoal 
        ^parameters <p>)
   (<p> ^object <cand>
        ^property <prop>)
}

sp {find-object*apply*create-measure-object-subgoal
   (state <s> ^name find-object
              ^operator <o>
              ^action-goal <goal>)
   (<o> ^name create-measure-object-subgoal
        ^parameters <p>)
-->
   (<goal> ^action-goal <subgoal>)
   (<subgoal> ^name measure-object
              ^parameters <p>)
}