sp {find-object*propose*create-move-object-subgoal
   (state <s> ^name find-object
             -^moved-to-goal true
              ^matching-info.good-match <cand>
              ^goal-location <goal>
             -^action-goal.action-goal)
-->
   (<s> ^operator <o> + > =)
   (<o> ^name create-move-object-subgoal
        ^parameters <p>)
   (<p> ^object <cand>
        ^destination <goal>)
}

sp {find-object*apply*create-move-object*subgoal
   (state <s> ^name find-object
              ^operator <o>
              ^action-goal <goal>)
   (<o> ^name create-move-object-subgoal
        ^parameters <p>)
-->
   (<goal> ^action-goal <subgoal>)
   (<subgoal> ^name move-object
              ^parameters <p>)
}

sp {find-object*apply*move-object*mark*moved-to-goal
   (state <s> ^name find-object
              ^operator <o>
              ^action-goal <goal>
              ^goal-location <goal-loc>)
   (<o> ^name move-object
        ^action-goal.achieved true
        ^destination <goal-loc>)
-->
   (<goal> ^moved-to-goal true)
}
   
   