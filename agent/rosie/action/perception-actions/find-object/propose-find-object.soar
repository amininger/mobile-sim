sp {action*propose*create-find-object-goal
   (state <s> ^name action
              ^topstate.interaction.stack.segment.purpose <p>
             -^action-goal-stack.action-goal)
   (<p> ^type action-command
        ^parameters.verb <verb>)
   (<verb> ^word find
           ^direct-object.object <obj>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name create-find-object-goal
        ^parameters <params>)
   (<params> ^object <obj>
             ^verb <verb>)
}

sp {action*apply*create-find-object-subgoal
   (state <s> ^name action
              ^operator <o>
              ^action-goal-stack <stack>)
   (<o> ^name create-find-object-goal
        ^parameters <p>)
-->
   (<stack> ^action-goal <goal>)
   (<goal> ^name find-object
           ^parameters <p>)
}
