sp {move-object*propose*create-clear-object-subgoal
   (state <s> ^name move-object
              ^object-to-move <obj>
              ^object-is-clear false
             -^action-goal.action-goal.name clear-object
             -^action-goal.achieved-subgoal.name clear-object)
-->
   (<s> ^operator <o> + >)
   (<o> ^name create-clear-object-subgoal
        ^parameters <p>)
   (<p> ^object <obj>)
}

sp {move-object*apply*create-clear-object-subgoal
   (state <s> ^name move-object
              ^operator <o>
              ^action-goal <goal>)
   (<o> ^name create-clear-object-subgoal
        ^parameters <p>)
-->
   (<goal> ^action-goal <subgoal>)
   (<subgoal> ^name clear-object
              ^parameters <p>)
}