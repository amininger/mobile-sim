sp {measure-object*propose*mark-measure-time
   (state <s> ^name measure-object
             -^measure-after-time 
             -^action-goal.action-goal)
-->
   (<s> ^operator <o> + >)
   (<o> ^name mark-measure-time)
}

sp {measure-object*apply*mark-measure-time
   (state <s> ^name measure-object
              ^operator.name mark-measure-time
               ^io.input-link.time.microseconds <t>
               ^action-goal <goal>)
-->
   (<goal> ^measure-after-time <t>)
}