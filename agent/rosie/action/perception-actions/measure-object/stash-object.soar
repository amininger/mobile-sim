sp {measure-object*propose*create-stash-object-subgoal
   (state <s> ^name measure-object
              ^object-on-location true
             -^action-goal.action-goal.name stash-object
             -^action-goal.achieved-subgoal.name stash-object
              ^object-to-measure <obj>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name create-stash-object-subgoal
        ^parameters.object <obj>)
}

sp {measure-object*apply*create-stash-object-subgoal
   (state <s> ^name measure-object
              ^action-goal <goal>
              ^operator <o>)
   (<o> ^name create-stash-object-subgoal
        ^parameters <p>)
-->
   (<goal> ^action-goal <subgoal>)
   (<subgoal> ^name stash-object
              ^parameters <p>)
}