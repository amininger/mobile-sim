sp {measure-object*propose*create-move-object-subgoal
   (state <s> ^name measure-object
              ^object-on-location false
              ^object-to-measure <obj>
              ^location-to-measure-at <loc>
             -^action-goal.achieved-subgoal.name move-object
             -^action-goal.action-goal.name move-object)
-->
   (<s> ^operator <o> + >)
   (<o> ^name create-move-object-subgoal
        ^parameters <p>)
   (<p> ^object <obj>
        ^destination <loc>)
}

sp {measure-object*apply*create-move-object-subgoal
   (state <s> ^name measure-object
              ^operator <o>
              ^action-goal <goal>)
   (<o> ^name create-move-object-subgoal
        ^parameters <p>)
-->
   (<goal> ^action-goal <subgoal>)
   (<subgoal> ^name move-object
              ^parameters <p>)
}