### indicate status of completed action
sp {propose*write*action*status*topstate
   (state <s> ^superstate <ss>
   	      ^impasse no-change
	      ^attribute state)
   (<ss> ^indexed-action <act>
   	 ^name action)
   (<act> ^applied yes)
-->
   (<ss> ^operator <op> + !)
   (<op> ^name indicate-completed-action
   	 ^indexed-action <act>)
}


sp {apply*write*action*status*topstate
   (state <s> ^name action
   	      ^topstate.interaction.status <stat>
	      ^operator <op>
	      ^lexical-information <lex>)
   (<op> ^name indicate-completed-action
   	 ^indexed-action <act>)
-->
   (<stat> ^performed <performed>)
   (<performed> ^action-event <ae>)
   (<ae> ^type <lex>)
}
   
sp {action*apply*indicate-completed-action*put-down*remove*grabbed
   (state <s> ^name action
              ^operator.name indicate-completed-action
              ^io.input-link.self <self>
              ^topstate <top>)
   (<self> ^grabbed-object -1)
   (<top> ^grabbed <obj>)
-->
   (<top> ^grabbed <obj> -)
}
