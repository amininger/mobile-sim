### implement action-model for pick-up 
sp {apply*pickup*object*action-model
   (state <s> ^operator <op>
   	      -^io)
   (<op> ^name op_pick-up-object
   	 ^argument1 <obj>)
-->
   (<s> ^grabbed <obj>)
}
              
sp {topstate*pickup*move*object*svs
   (state <s> ^name action
              ^io.input-link.self <self>
              ^io.output-link <ol>
              ^topstate.svs.command <cmd>
              ^operator <op>)
   (<op> ^name op_pick-up-object
         ^argument1.belief-obj.id <id>)
   (<self> ^pose <pose>)
   (<pose> ^x <x> ^y <y> ^z <z>)
-->
   (<cmd> ^property <xp> <yp> <zp>)
   (<xp> ^id <id>
         ^property px
         ^value <x>)
   (<yp> ^id <id>
         ^property py
         ^value <y>)
   (<zp> ^id <id>
         ^property pz
         ^value <z>)
}
