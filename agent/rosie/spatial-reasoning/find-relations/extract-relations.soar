   (<N7> ^type node ^id <id>)
   (<E12> ^a <N7> ^b <E13> ^type <y> ^bot <by> ^top <ty>)
   (<E13> ^a <N7> ^b <belief-nodes> ^type <z> ^bot <bz> ^top <tz>)
   (<belief-nodes> ^type has-property
                   ^a.type all_nodes
                   ^property-name object-source
                   ^property-value belief)
}

