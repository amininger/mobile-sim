sp {evaluate-shrunken-object*propose*intersect-command
   (state <s> ^name evaluate-shrunken-object
              ^monitor-obj.belief-obj.id <id>
             -^intersect-command.result)
-->
   (<s> ^operator <o> +)
   (<o> ^name intersect-command
        ^category perception
        ^object-id <id>
        ^candidate-set <perc-objs>)
   (<perc-objs> ^type has-property
                ^a.type all_nodes
                ^property-name object-source
                ^property-value perception)
}
