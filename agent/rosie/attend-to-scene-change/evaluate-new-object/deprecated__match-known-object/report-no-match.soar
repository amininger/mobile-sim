sp {match-known-object*propose*report-no-match
   (state <s> ^name match-known-object)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-no-match
        ^category perception)
}

sp {match-known-object*apply*report-no-match
   (state <s> ^name match-known-object
              ^operator.name report-no-match
              ^superstate <ss>)
-->
   (<ss> ^matches-known-object none)
}