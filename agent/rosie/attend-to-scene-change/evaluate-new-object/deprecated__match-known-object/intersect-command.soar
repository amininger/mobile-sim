sp {match-known-object*propose*intersect-command
   (state <s> ^name match-known-object
              ^monitor-obj.perception-obj.id <id>
             -^intersect-command)
-->
   (<s> ^operator <o> +)
   (<o> ^name intersect-command
        ^category perception
        ^object-id <id>
        ^candidate-set <cand>)
   (<cand> ^type has-property
           ^a.type all_nodes
           ^property-name object-source
           ^property-value belief)
}

