sp {create-new-object*propose*copy-name-to-object
   (state <s> ^name create-new-object
              ^new-object <new-obj>
              ^perception-obj.name <name>)
   (<new-obj> ^property <name-prop>)
   (<name-prop> ^name name
               -^value.word <name>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name copy-name-to-object
        ^category perception
        ^obj-name <name>)
}

sp {create-new-object*apply*copy-name-to-object
   (state <s> ^name create-new-object
              ^operator <o>
              ^new-object <new-obj>)
   (<o> ^name copy-name-to-object
        ^obj-name <name>)
   (<new-obj> ^property <name-prop>)
   (<name-prop> ^name name)
-->
   (<name-prop> ^value <val>)
   (<val> ^word <name>
          ^label <name>
          ^confidence high)
}