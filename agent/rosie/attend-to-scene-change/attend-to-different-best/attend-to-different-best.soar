sp {attend-to-scene-change*apply*attend-to-different-best
   (state <s> ^name attend-to-scene-change
              ^operator <o>)
   (<o> ^name attend-to-different-best
        ^property.wm-prop <wm-prop>
        ^best-hyp <h>)
   (<h> ^label <new-label>
        ^conf <new-conf>)
   (<wm-prop> ^value <value>)
-->
   (<wm-prop> ^value <value> -
              ^value <new-val>)
   (<new-val> ^label <new-label>
              ^confidence <new-conf>)
}