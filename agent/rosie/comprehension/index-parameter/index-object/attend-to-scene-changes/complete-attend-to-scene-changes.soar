sp {attend-to-scene-changes*propose*complete-attend-to-scene-changes
   (state <s> ^name attend-to-scene-changes)
-->
   (<s> ^operator <o> + <)
   (<o> ^name complete-attend-to-scene-changes
        ^category perception)
}

sp {attend-to-scene-changes*apply*complete-attend-to-scene-changes
   (state <s> ^name attend-to-scene-changes
              ^operator.name complete-attend-to-scene-changes
              ^superstate <ss>)
-->
   (<ss> ^attended-to-scene-changes true)
}