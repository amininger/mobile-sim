sp {augment-existing-mapping*store*argument-substructure
   (state <s> ^name create-new-mapping
   	      ^superstate <ss>
	      ^smem.command <scomm>)
   (<ss> ^operator <sop>)
   (<sop> ^smem-pointer <sp>
   	  ^argument <arg>)
-->
  (<sp> ^<arg> <narg>)
  (<scomm> ^store <sp> <narg>)
  (<ss> ^new-map <nmap>)
  (<nmap> ^argument <arg>
	  ^smem-pointer <sp>)
}