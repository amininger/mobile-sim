sp {initialize-comprehend-action-command*propose*deletions
   (state <s> ^name initialize-comprehend-action-command
   	      -^deletion-completed true)
-->
   (<s> ^operator <op> +)
   (<op> ^name delete-not-indexed-parameters)
}

sp {initialize-comprehend-action-command*propose*elaborations
   (state <s> ^name initialize-comprehend-action-command
   	      ^deletion-completed true
	      -^elaborated true)
-->
   (<s> ^operator <op> +)
   (<op> ^name elaborate-indexable-parameters)
}